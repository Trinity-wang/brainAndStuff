import matplotlib.pyplot as plt

plt.matshow(self.layer[0].S[0], cmap=plt.cm.gray)
plt.title('EE')



plt.matshow(self.layer[0].S[1], cmap=plt.cm.gray)
plt.title('IE')



plt.matshow(self.layer[1].S[0], cmap=plt.cm.gray)
plt.title('EI')



plt.matshow(self.layer[1].S[1], cmap=plt.cm.gray)
plt.title('II')
plt.show()






# scaling factor 
self.layer[1].factor[0] == 50
self.layer[1].factor[1] == 1 
self.layer[0].factor[1] == 2
self.layer[0].factor[0] == 17
